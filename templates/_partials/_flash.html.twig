{% for label,messages in app.flashes %}

{% for message in messages %}
<div class="alert alert-{{label}} alert-dismissible" role="alert" style="width:300px;position:absolute;margin-top:10px;margin-left:1000px">


<button class="btn-close" data-bs-dismiss="alert" aria-label="close" ></button>
<div class="alert-message">
{{message | raw}}
</div>
    </div>
{% endfor %}
    
{% endfor %}